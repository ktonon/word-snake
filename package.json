{
  "name": "word_snake",
  "version": "1.0.0",
  "description": "Asynchronous word search game",
  "main": "index.js",
  "scripts": {
    "build": "node scripts/write-config.js && webpack",
    "debug:api": "supervisor local-server.js",
    "debug:app": "node scripts/write-config.js && webpack-dev-server",
    "start": "npm run debug:api & npm run debug:app",
    "lint": "eslint .",
    "test": "npm run lint",
    "deploy:api": "serverless deploy",
    "deploy:app": "npm run build && node node_modules/s3-website/s3-website.js deploy",
